{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "src",
    "jsx": "react-jsx",
    "types": ["astro/client"],
    "paths": {
      "@/*": ["*"],
      "@src/*": ["src/*"],
      "@lib/*": ["lib/*"],
      "@components/*": ["components/*"],
      "@content/*": ["content/*"],
      "@layouts/*": ["layouts/*"],
      "@styles/*": ["styles/*"],
      "@pages/*": ["pages/*"],
      "@support/*": ["support/*"],
      "@images/*": ["images/*"]
    }
  }
}
