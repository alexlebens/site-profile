---
import ImageTheme from '@components/ui/images/ImageTheme.astro';

const { srcLight, srcDark, alt } = Astro.props;

---

<ImageTheme
  srcLight={srcLight}
  srcDark={srcDark}
  alt={alt}
  style='color: transparent; width: 48px; height: 48px; object-fit: contain; max-height: 100%; max-width: 100%;'
  draggable="false"
  loading="lazy"
  width="48"
  height="48"
/>
